\setbeamercolor{background canvas}{bg=ttbeamer@background}%
\setbeamercolor{title author}{fg=ttbeamer@bottom}
\setbeamercolor{title institute}{fg=ttbeamer@bottom}

\setbeamerfont{structure}{series=\mdseries}
\setbeamercolor{structure}{fg=ttbeamer@structure}

% \setbeamerfont{alerted text}{series=\bfseries}
