
\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamerthemeFGMSO}[2023/01/21]
\mode<presentation>
\RequirePackage{etoolbox}
\RequirePackage{ifthen}
\RequirePackage{zref-abspage}

\def\ttbeamer@pathname@patch{\ttbeamer@pathname@base patch/}
\def\ttbeamer@filename@patch{%
  \ttbeamer@pathname@patch ttbeamer-\ttbeamer@option@workplace}
\newlength\ttbeamer@length@margin
\ttbeamer@length@margin=0.7cm
\newlength\ttbeamer@length@footlinebottompadding
\ttbeamer@length@footlinebottompadding=.163cm
\newlength\ttbeamer@length@footlineheight
\ttbeamer@length@footlineheight=.4cm
\newlength\ttbeamer@length@logoheight
\ttbeamer@length@logoheight=1.4cm
\definecolor{ttbeamer@structure}{RGB}{43,58,68}
\definecolor{ttbeamer@base}{cmyk}{60, 40, 40, 100} % Black
\definecolor{ttbeamer@bottom}{cmyk}{0,0,0,0} % White
\definecolor{ttbeamer@background}{cmyk}{0,0,0,0} % White {RGB}{242, 242, 242}
\def\bottomfont{\sffamily}
\setbeamersize{%
  text margin left=\ttbeamer@length@margin,
  text margin right=\ttbeamer@length@margin}
\setbeamertemplate{frametitle}{%
  \vtop{{%
      \usebeamerfont{frametitle}{\insertframetitle}%
      \ifx\insertframesubtitle\@empty\else\\
        \usebeamerfont{framesubtitle}{\insertframesubtitle}
      \fi}
    \vfill}}

%
% NAVIGATION SYMBOLS
%
\setbeamertemplate{navigation symbols}{}

%
% HEADLINE
%
\setbeamertemplate{headline}{}

%
% NO FRAMETITLE NUMBERS WHEN ALLOWFRAMEBREAKS
%
\setbeamertemplate{frametitle continuation}{}

%
% FOOTLINE ON SLIDES
%
\setbeamertemplate{footline}{%
  \hskip\ttbeamer@length@margin
  \usebeamercolor[fg]{author in head/foot}%
    \usebeamerfont{author in head/foot}\insertshortauthor
  \usebeamercolor[fg]{structure}%
    \usebeamerfont{structure}\ \lower1pt\hbox{\normalsize$\cdot$}\
  \usebeamercolor[fg]{title in head/foot}%
    \usebeamerfont{author in head/foot}\insertshorttitle
  \usebeamercolor[fg]{structure}%
    \usebeamerfont{structure}\ \lower1pt\hbox{\normalsize$\cdot$}\
  \usebeamercolor[fg]{date in head/foot}%
    \usebeamerfont{date in head/foot}\insertshortdate\hfill
  \usebeamercolor[fg]{page number in head/foot}%
    \usebeamerfont{author in head/foot}\insertframenumber\ /
    \inserttotalframenumber
  \hskip\ttbeamer@length@margin
  \vrule width 0pt height \ttbeamer@length@footlineheight
         depth \ttbeamer@length@footlinebottompadding}

%
% TITLE PAGE
%
\setbeamertemplate{title page}{%
  \vskip\ttbeamer@length@margin
  \ifdefempty{\institutegraphic}{}{\includegraphics[height=\instituteheight,keepaspectratio]{\institutegraphic}}
  \ifdefempty{\projectgraphic}{}{\hfill\includegraphics[height=\projectheight,keepaspectratio]{\projectgraphic}}
  \vskip0pt plus 6filll
  {\usebeamerfont{title}\usebeamercolor[fg]{title}\inserttitle\par}%
  \vskip0pt plus 1filll

  {\usebeamerfont{subtitle}\usebeamercolor[fg]{subtitle}\insertsubtitle}%
  \vskip0pt plus 4filll

  {\usebeamerfont{author}\usebeamercolor[fg]{title author}\insertauthor}%

  {\usebeamerfont{institute}\usebeamercolor[fg]{title institute}\insertinstitute}%
  \vskip0pt plus 4filll


  {\usebeamerfont{date} %\insertshorttitle \usebeamerfont{structure}\ \lower1pt\hbox{\Large$\cdot$}\ 
  \usebeamerfont{date}\insertdate}%
  \vskip\ttbeamer@length@margin


  \ifdefempty{\fundinggraphic}{}{\vspace{-\fundingheight}\vspace{-\ttbeamer@length@margin}\hfill\includegraphics[height=\fundingheight,keepaspectratio]{\fundinggraphic}\vskip\ttbeamer@length@margin}

  }
\setbeamertemplate{bibliography item}[text]


% SECTION SEPARATOR
% 
\AtBeginSection[]{
    \begin{frame}[noframenumbering,plain]\frametitle{}
        \centering
        \usebeamerfont{title}\insertsectionhead
    \end{frame}
}




